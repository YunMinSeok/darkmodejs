{
  "name": "darkmodejs",
  "version": "1.0.0",
  "description": "The darkmode package",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YunMinSeok/darkmodejs.git"
  },
  "keywords": [
    "darkmode",
    "dark",
